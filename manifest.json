{
  "manifest_version": 2,
  "name": "Golinks",
  "description": "Your personal, cloud-synced golinks.",
  "version": "1.0.0",

  "permissions": ["storage", "activeTab"],

  "omnibox": {
    "keyword": "go"
  },

  "background": {
    "persistent": false,
    "scripts": [
      "js/store.js",
      "js/omnibox.js"
    ]
  },

  "web_accessible_resources": [
    "edit.html"
  ]
}
